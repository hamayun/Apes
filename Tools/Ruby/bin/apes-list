#!/usr/bin/ruby

require 'apes/id'
require 'apes/parse'

def program_usage
  puts 'Usage: apes-list'
end

if ARGV.count != 0 then
  program_usage
else
  begin
    APELibraryParser.getComponentList.each { |e| puts e.id.to_s }
  rescue RuntimeError
    puts '[list] Variable APES_COMPONENT_PATH not defined.'
  end
end

