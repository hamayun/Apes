# Example robodoc.rc

items:
	SUMMARY
	SYNOPSIS
	FUNCTION
	ARGUMENTS
	RESULT
	SIDE EFFECTS
	HISTORY
	BUGS
	TODO
	EXAMPLE
	NOTES

source items:
	SYNOPSIS

ignore items:
	HISTORY
	BUGS

options:
	--html
	--multidoc
	--index
	--tabsize 4
	--sections
	--cmode
  --syntaxcolors
	--nogeneratedwith
	--source_line_numbers
	--nopre
  --masterindex "Index Page,index"
  --sourceindex "Source files,sources"

ignore files:
	README
	CVS
	*.bak
	*~
	"a test_*"
	.*
	Makefile

accept files:
	*.c
	*.h
